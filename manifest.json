{
  "manifest_version": 3,
  "name": "Thrive - Distraction Free Internet",
  "description": "Removes the feed from Youtube, Instagram and Amazon, so that you can browse distraction free",
  "version": "1.1.3",
  "icons": {
    "16": "images/thrive_icon_16.png",
    "32": "images/thrive_icon_32.png",
    "48": "images/thrive_icon_48.png",
    "64": "images/thrive_icon_64.png",
    "128": "images/thrive_icon_128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "tabs",
    "webNavigation",
    "storage"
  ],
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://m.youtube.com/*",
    "https://www.instagram.com/*",
    "https://www.amazon.com/*",
    "https://www.amazon.ca/*",
    "https://www.amazon.ae/*",
    "https://www.amazon.in/*",
    "https://www.amazon.de/*",
    "https://www.amazon.co.uk/*",
    "https://www.amazon.com.br/*",
    "https://www.amazon.com.au/*",
    "https://www.amazon.co.jp/*",
    "https://www.amazon.fr/*",
    "https://www.amazon.it/*",
    "https://www.amazon.es/*",
    "https://www.amazon.com.mx/*",
    "https://www.amazon.sa/*",
    "https://www.amazon.sg/*",
    "https://www.amazon.nl/*",
    "https://www.amazon.com.tr/*",
    "https://www.amazon.pl/*",
    "https://www.amazon.se/*",
    "https://www.amazon.eg/*",
    "https://www.amazon.com.be/*"
  ]
}
